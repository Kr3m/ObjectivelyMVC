{
  "class": "Panel",
  "frame": [
    50,
    50,
    0,
    0
  ],
  "contentView": {
    "subviews": [
      {
        "class": "TabView",
        "identifier": "tabView",
        "tabs": [
          {
            "identifier": "controls",
            "label": {
              "text": {
                "text": "Controls"
              }
            },
            "view": {
              "class": "StackView",
              "spacing": 12,
              "subviews": [
                {
                  "class": "Button",
                  "identifier": "button",
                  "style": "ControlStyleDefault",
                  "title": {
                    "text": "This is a Button"
                  }
                },
                {
                  "class": "Input",
                  "control": {
                    "class": "Checkbox",
                    "identifier": "checkbox"
                  },
                  "label": {
                    "text": {
                      "text": "This is a checkbox:"
                    }
                  }
                },
                {
                  "class": "TextView",
                  "identifier": "textView",
                  "defaultText": "This is a TextView"
                },
                {
                  "class": "Select",
                  "identifier": "select"
                },
                {
                  "class": "Slider",
                  "identifier": "slider",
                  "min": 0,
                  "max": 10,
                  "value": 5
                }
              ]
            }
          },
          {
            "identifier": "tables",
            "label": {
              "text": {
                "text": "Tables"
              }
            },
            "view": {
              "class": "StackView",
              "subviews": [
                {
                  "class": "TableView",
                  "identifier": "tableView",
                  "autoresizingMask": "ViewAutoresizingWidth",
                  "columns": [
                    {
                      "identifier": "Column A"
                    },
                    {
                      "identifier": "Column B"
                    },
                    {
                      "identifier": "Column C"
                    }
                  ],
                  "selection": "ControlSelectionMultiple"
                }
              ]
            }
          },
          {
            "identifier": "collections",
            "label": {
              "text": {
                "text": "Collections"
              }
            },
            "view": {
              "class": "StackView",
              "subviews": [
                {
                  "class": "CollectionView",
                  "identifier": "collectionView",
                  "constraints": [
                    "w >= 300",
                    "h >= 260"
                  ],
                  "selection": "ControlSelectionMultiple",
                  "itemSize": [
                    64,
                    48
                  ]
                }
              ]
            }
          },
          {
            "identifier": "colors",
            "label": {
              "text": {
                "text": "Colors"
              }
            },
            "view": {
              "class": "StackView",
			  "spacing": 10,
              "subviews": [
                {
                  "class": "Box",
                  "label": {
                    "text": {
                      "text": "RGB Color Picker"
                    }
                  },
                  "contentView": {
                    "subviews": [
                      {
                        "class": "ColorPicker",
                        "identifier": "colorPicker"
                      }
                    ]
                  }
                },
                {
                  "class": "Box",
                  "label": {
                    "text": {
                      "text": "HSV Color Picker"
                    }
                  },
                  "contentView": {
                    "subviews": [
                      {
                        "class": "HSVColorPicker",
                        "identifier": "hsvColorPicker"
                      }
                    ]
                  }
                }
              ]
            }
          }
        ]
      }
    ]
  },
  "accessoryView": {
    "hidden": false,
    "subviews": [
      {
        "class": "Button",
        "identifier": "cancel",
        "title": {
          "text": "Cancel"
        }
      },
      {
        "class": "Button",
        "identifier": "apply",
        "title": {
          "text": "Apply"
        }
      }
    ]
  }
}
