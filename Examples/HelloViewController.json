{
	"class": "Panel",
	"frame": [50, 50, 0, 0],
	"contentView": {
		"subviews": [{
			"class": "TabView",
			"identifier": "tabView",
			"tabs": [{
				"identifier": "controls",
				"label": {
					"text": {
						"text": "Controls"
					}
				},
				"view": {
					"class": "StackView",
					"spacing": 12,
					"subviews": [{
						"class": "Button",
						"identifier": "button",
						"style": "ControlStyleDefault",
						"title": {
							"text": "This is a Button"
						}
					}, {
						"class": "Input",
						"control": {
							"class": "Checkbox",
							"identifier": "checkbox"
						},
						"label": {
							"text": {
								"text": "This is a checkbox:"
							}
						}
					}, {
						"class": "TextView",
						"identifier": "textView",
						"defaultText": "This is a TextView"
					}, {
						"class": "Select",
						"identifier": "select"
					}, {
						"class": "Slider",
						"identifier": "slider",
						"min": 0.0,
						"max": 10.0,
						"value": 5.0
					}]
				}
			}, {
				"identifier": "tables",
				"label": {
					"text": {
						"text": "Tables"
					}
				},
				"view": {
					"class": "StackView",
					"subviews": [{
						"class": "TableView",
						"identifier": "tableView",
						"autoresizingMask": "ViewAutoresizingWidth",
						"columns": [{
							"identifier": "One"
						}, {
							"identifier": "Two"
						}, {
							"identifier": "Three"
						}],
						"selection": "ControlSelectionMultiple"
					}]
				}
			}, {
				"identifier": "collections",
				"label": {
					"text": {
						"text": "Collections"
					}
				},
				"view": {
					"class": "StackView",
					"subviews": [{
						"class": "CollectionView",
						"identifier": "collectionView",
						"autoresizingMask": "ViewAutoresizingFill",
						"selection": "ControlSelectionMultiple",
						"axis": "CollectionViewAxisHorizontal",
						"itemSize": [64, 48]
					}]
				}
			}]
		}]
	},
	"accessoryView": {
		"hidden": false,
		"subviews": [{
			"class": "Button",
			"title": {
				"text": "Cancel"
			}
		}, {
			"class": "Button",
			"title": {
				"text": "Apply"
			}
		}]
	}
}
